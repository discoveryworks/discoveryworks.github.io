  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "Comment",
        "scope": [
          "comment",
          "punctuation.definition.comment"
        ],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#9299a0"
        }
      },
      {
        "name": "String",
        "scope": [
          "string",
          "punctuation.definition.string"
        ],
        "settings": {
          "foreground": "#2a70a2"
        }
      },
      {
        "name": "Number",
        "scope": [
          "constant.numeric"
        ],
        "settings": {
          "foreground": "#dc872e"
        }
      },
      {
        "name": "Built-in constant",
        "scope": [
          "constant.language"
        ],
        "settings": {
          "foreground": "#dc872e"
        }
      },
      {
        "name": "User-defined constant",
        "scope": [
          "constant.character",
          "constant.other"
        ],
        "settings": {
          "foreground": "#dc872e"
        }
      },
      {
        "name": "Variable",
        "scope": [
          "variable"
        ],
        "settings": {
          "foreground": "#2735d3"
        }
      },
      {
        "name": "Keyword",
        "scope": [
          "keyword"
        ],
        "settings": {
          "foreground": "#389900"
        }
      },
      {
        "name": "Storage",
        "scope": [
          "storage"
        ],
        "settings": {
          "foreground": "#389900"
        }
      },
      {
        "name": "Storage type",
        "scope": [
          "storage.type"
        ],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#2735d3"
        }
      },
      {
        "name": "Class name",
        "scope": [
          "entity.name.class"
        ],
        "settings": {
          "foreground": "#2735d3"
        }
      },
      {
        "name": "Function name",
        "scope": [
          "entity.name.function"
        ],
        "settings": {
          "foreground": "#2735d3"
        }
      },
      {
        "name": "Function argument",
        "scope": [
          "variable.parameter"
        ],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#caa616"
        }
      },
      {
        "name": "Tag name",
        "scope": [
          "entity.name.tag"
        ],
        "settings": {
          "foreground": "#2735d3"
        }
      },
      {
        "name": "Tag attribute",
        "scope": [
          "entity.other.attribute-name"
        ],
        "settings": {
          "foreground": "#389900"
        }
      },
      {
        "name": "Library function",
        "scope": [
          "support.function"
        ],
        "settings": {
          "foreground": "#2735d3"
        }
      },
      {
        "name": "Library constant",
        "scope": [
          "support.constant"
        ],
        "settings": {
          "foreground": "#dc872e"
        }
      },
      {
        "name": "Library class/type",
        "scope": [
          "support.type",
          "support.class"
        ],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#2735d3"
        }
      },
      {
        "name": "Library variable",
        "scope": [
          "support.other.variable"
        ],
        "settings": {
          "foreground": "#2735d3"
        }
      },
      {
        "name": "Invalid",
        "scope": [
          "invalid"
        ],
        "settings": {
          "background": "#dc872e",
          "foreground": "#f7fde2"
        }
      },
      {
        "name": "Invalid deprecated",
        "scope": [
          "invalid.deprecated"
        ],
        "settings": {
          "background": "#caa616",
          "foreground": "#f7fde2"
        }
      },
      {
        "name": "JSON String",
        "scope": [
          "meta.structure.dictionary.json string.quoted.double.json"
        ],
        "settings": {
          "foreground": "#86b300"
        }
      },
      {
        "name": "diff.header",
        "scope": [
          "meta.diff",
          "meta.diff.header"
        ],
        "settings": {
          "foreground": "#9299a0"
        }
      },
      {
        "name": "diff.deleted",
        "scope": [
          "markup.deleted"
        ],
        "settings": {
          "foreground": "#dc872e"
        }
      },
      {
        "name": "diff.inserted",
        "scope": [
          "markup.inserted"
        ],
        "settings": {
          "foreground": "#389900"
        }
      },
      {
        "name": "diff.changed",
        "scope": [
          "markup.changed"
        ],
        "settings": {
          "foreground": "#86b300"
        }
      },
      {
        "name": "CSS Classes",
        "scope": [
          "entity.other.attribute-name.class.css"
        ],
        "settings": {
          "foreground": "#86b300"
        }
      },
      {
        "name": "CSS IDs",
        "scope": [
          "entity.other.attribute-name.id.css"
        ],
        "settings": {
          "foreground": "#86b300"
        }
      },
      {
        "name": "CSS Properties",
        "scope": [
          "support.type.property-name.css"
        ],
        "settings": {
          "foreground": "#2735d3"
        }
      },
      {
        "name": "CSS Property Values",
        "scope": [
          "support.constant.property-value.css",
          "constant.numeric.css"
        ],
        "settings": {
          "foreground": "#2a70a2"
        }
      },
      {
        "name": "Markdown Headings",
        "scope": [
          "markup.heading"
        ],
        "settings": {
          "fontStyle": "bold",
          "foreground": "#2735d3"
        }
      },
      {
        "name": "Markdown Links",
        "scope": [
          "markup.underline.link"
        ],
        "settings": {
          "foreground": "#2a70a2"
        }
      },
      {
        "name": "Markdown Bold",
        "scope": [
          "markup.bold"
        ],
        "settings": {
          "fontStyle": "bold",
          "foreground": "#caa616"
        }
      },
      {
        "name": "Markdown Italic",
        "scope": [
          "markup.italic"
        ],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#caa616"
        }
      }
    ]
  }
}