  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "Comment",
        "scope": [
          "comment",
          "punctuation.definition.comment"
        ],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#9e92a0"
        }
      },
      {
        "name": "String",
        "scope": [
          "string",
          "punctuation.definition.string"
        ],
        "settings": {
          "foreground": "#822aa2"
        }
      },
      {
        "name": "Number",
        "scope": [
          "constant.numeric"
        ],
        "settings": {
          "foreground": "#4bdc2e"
        }
      },
      {
        "name": "Built-in constant",
        "scope": [
          "constant.language"
        ],
        "settings": {
          "foreground": "#4bdc2e"
        }
      },
      {
        "name": "User-defined constant",
        "scope": [
          "constant.character",
          "constant.other"
        ],
        "settings": {
          "foreground": "#4bdc2e"
        }
      },
      {
        "name": "Variable",
        "scope": [
          "variable"
        ],
        "settings": {
          "foreground": "#d327ab"
        }
      },
      {
        "name": "Keyword",
        "scope": [
          "keyword"
        ],
        "settings": {
          "foreground": "#009991"
        }
      },
      {
